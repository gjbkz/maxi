{
    "compilerOptions": {
        "target": "es2017",
        "resolveJsonModule": true,
        "strict": true,
        "removeComments": true,
        "declaration": true,
        "sourceMap": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
    }
}
