{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "declaration": true,
        "strict": true,
        "removeComments": true,
        "sourceMap": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true
    }
}
