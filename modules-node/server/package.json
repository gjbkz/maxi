{
    "name": "@maxi-js/server",
    "author": {
        "name": "Kei Ito",
        "email": "kei.itof@gmail.com",
        "url": "https://github.com/kei-ito"
    },
    "homepage": "https://github.com/kei-ito/maxi/tree/master/modules-node/server",
    "repository": "https://github.com/kei-ito/maxi",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "version": "0.0.0",
    "license": "Apache-2.0",
    "main": "lib/index.js",
    "files": [
        "lib"
    ],
    "scripts": {
        "cleanup": "rimraf lib",
        "build:layer:cleanup": "rimraf layer",
        "build:layer:mkdir": "make-dir layer/nodejs",
        "build:layer:cpr": "cp -r node_modules layer/nodejs/node_modules",
        "build:layer": "run-s build:layer:cleanup build:layer:mkdir build:layer:cpr",
        "build:tsc": "tsc",
        "build": "run-p build:layer build:tsc",
        "test": "ava",
        "dev:layer": "npm run build:layer",
        "dev:sam": "sam local start-api --region ap-northeast-1",
        "dev:tsc": "tsc --watch --preserveWatchOutput",
        "dev": "run-p dev:*"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.24",
        "@types/node": "11.13.8"
    },
    "dependencies": {
        "@maxi-js/catalog": "^0.0.0",
        "@maxi-js/data-viewer": "^0.0.0",
        "@maxi-js/net-tools": "^0.0.0",
        "@maxi-js/stream-tools": "^0.0.0",
        "@maxi-js/string-tools": "^0.0.0"
    }
}
